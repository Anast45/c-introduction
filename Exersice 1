//SOLUTION///////////////////////////////////////////////////////////////////////////////////////////////////
#include<stdio.h>
int main()
{
int x,y;   
  
  printf("give me two integers\n");
  scanf("%d%d",&x,&y);         
  
  if(x>y){printf("%d\n",x);}
  if(x<y){printf("%d\n",y);}
  else{printf("x=y");}
  //or if(x==y){printf("x=y");}
  
  return 0; 
}
